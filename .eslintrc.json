{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": "airbnb",
  "plugins": [
    "react-native"
  ],
  "rules": {
    "react-native/no-unused-styles": 2,
    "react-native/split-platform-components": 2,
    "linebreak-style":0,
    "react/jsx-filename-extension": [
      "error", {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}
