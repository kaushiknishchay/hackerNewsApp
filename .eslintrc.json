{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules":true,
    "arrowFunctions":true,
    "classes":true,
    "spread":true
  },
  "extends": "airbnb",
  "plugins": [
    "react-native"
  ],
  "rules": {
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "react-native/no-unused-styles": 2,
    "react-native/split-platform-components": 2,
    "linebreak-style":0,
    "react/jsx-filename-extension": [
      "error", {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}
